<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/item1">
        <shape android:shape="ring" android:thicknessRatio="20.0" android:useLevel="true" android:innerRadius="16dp">
            <gradient android:startColor="#Cfffff" android:endColor="#Efffff" android:type="sweep" android:centerColor="#Dfffff" />
        </shape>
    </item>
    <item android:id="@+id/item2">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="ring" android:thicknessRatio="20.0" android:useLevel="true" android:innerRadius="16dp">
                <rotate android:fromDegrees="0.0" android:toDegrees="360.0" android:pivotX="50%" android:pivotY="50%" />
                <gradient android:startColor="#ffffff" android:endColor="#ffffff" android:type="sweep" android:centerColor="#ffffff" />
            </shape>
        </rotate>
    </item>
</layer-list>
